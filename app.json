{
  "expo": {
    "name": "SkipSpace",
    "slug": "skipspace-users-prod",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "skipspace-app",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#003b49"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/1ffaeec4-3751-4f8b-af74-ed6c8a5e4a77"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stmdeveloper.skipspaceapp",
      "buildNumber": "17"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#003b49"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.stmdeveloper.skipspaceapp",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow SkipSpace to use your location."
        }
      ],
      [
        "expo-updates",
        {
          "username": "stmdeveloper"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1ffaeec4-3751-4f8b-af74-ed6c8a5e4a77"
      }
    },
    "owner": "shoot-the-moon-ltd",
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
